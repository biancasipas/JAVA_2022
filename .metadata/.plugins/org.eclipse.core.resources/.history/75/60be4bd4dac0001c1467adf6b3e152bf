import javax.swing.JOptionPane;
public class c08ex05 {
	public static void main(String[] args) {
		//Programadora Bianca Maria
  int voto;
  int vota_primeiro1=0,vota_segundo2=0,vota_terceiro3=0;
  for (int repeticao = 1; repeticao <= 100; repeticao++) {
  voto = Integer.parseInt(JOptionPane.showInputDialog("Qual número de voto"));
		  switch(voto){
		   case 1:
		  JOptionPane.showMessageDialog(null,"Candidato Fulano votado");
		  vota_primeiro1++;
		  break;
		   case 2:
		  JOptionPane.showMessageDialog(null,"Candidato Ciclano votado");
		  vota_segundo2++;
		  break;
		   case 3:
		  JOptionPane.showMessageDialog(null,"Candidato Beltrano votado");
		  vota_terceiro3++;
		  break;
		   default:
		  }
  }
		  JOptionPane.showMessageDialog(null,
		  "Quantidade de número de candidato Fulano votado total: "+vota_primeiro1+"\n"+
		  "Quantidade de número de candidato Ciclano votado total: "+vota_segundo2+"\n"+
	      "Qauntidade de número de candidato Beltrano votado total: "+vota_terceiro3,
		  "ELEIÇÂO",
	      JOptionPane.INFORMATION_MESSAGE);
		  
		  }
		}
		 
	


